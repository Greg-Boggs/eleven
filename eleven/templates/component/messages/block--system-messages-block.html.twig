{{ attach_library(active_theme()~'/messages') }}
{#
/**
 * @file
 * Theme override for the messages block.
 *
 * Removes wrapper elements from block so that empty block does not appear when
 * there are no messages.
 *
 * Available variables:
 * - content: The content of this block.
 */
#}

{#

<a href="#" class="button hide-messages js-hide-messages" data-helptext="{{'Hide Messages'|t}}">{{'Hide Messages'|t}}</a>

#}




{% if content|render %}
  <div class="w-8">
    {{ content }}
  </div>
{% endif %}
